package myClass;

public class SenderEx {
	public static void main(String[] args) {
		
		ContentSender cs1 = new letterSender("SMS문자", "서수진", "4달라");
		cs1.sendMsg("슈화");
		
		System.out.println();
		
		ContentSender ls = new letterSender("SMS문자", "김민희", "3달라");
		ls.sendMsg("조미연");
		
		System.out.println();
		
		ContentSender cs2 = new letterSender("카톡", "송우기", "10달라");
		cs2.sendMsg("전소연");
		
	}
}
